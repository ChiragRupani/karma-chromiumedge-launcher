{
  "name": "@chiragrupani/karma-chromium-edge-launcher",
  "version": "1.0.0",
  "description": "A Karma plugin. Launcher for Chromium Edge - Dev, Canary and Beta",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "node node_modules/mocha/bin/mocha -r ts-node/register tests/**/*.test.ts",
    "coverage": "nyc npm run test",
    "package": "npm run build & npm pack"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/ChiragRupani"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ChiragRupani/karma-chromiumedge-launcher.git"
  },
  "keywords": [
    "karma-plugin",
    "karma-launcher",
    "Chromium Edge Dev",
    "Chromium Edge Canary",
    "Chromium Edge Beta"
  ],
  "author": "Chirag",
  "dependencies": {
    "which": "^1.3.1"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.2",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.4",
    "@types/sinon": "^7.0.13",
    "chai": "^4.2.0",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.3.0",
    "typescript": "^3.6.3"
  },
  "nyc": {
    "reporter": [
      "text"
    ],
    "temp-directory": "./coverage/.nyc_output",
    "exclude": "typings",
    "extension": [
      ".ts"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
