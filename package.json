{
  "name": "@chiragrupani/karma-chromium-edge-launcher",
  "version": "1.0.0",
  "description": "A Karma plugin. Launcher for Chromium Edge and Chromium Edge Canary.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "node node_modules/mocha/bin/mocha -r ts-node/register tests/**/*.test.ts",
    "testWithCoverage": "nyc -r lcov -e .ts -x \"*.test.ts\" mocha -r ts-node/register tests/**/*.test.ts && nyc report",
    "package": "npm run build & npm pack"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ChiragRupani/karma-chromiumedge-launcher.git"
  },
  "keywords": [
    "karma-plugin",
    "karma-launcher",
    "Chromium Edge Dev",
    "Chromium Edge Canary"
  ],
  "author": "Chirag",
  "dependencies": {
    "which": "^1.3.1"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "@types/node": "^12.0.3",
    "@types/sinon": "^7.0.12",
    "chai": "^4.2.0",
    "jasmine-node": "^3.0.0",
    "karma": "^4.1.0",
    "karma-mocha": "1.x || ^0.2.0",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "sinon": "^7.3.2",
    "ts-node": "^8.2.0",
    "typescript": "^3.5.1"
  }
}
